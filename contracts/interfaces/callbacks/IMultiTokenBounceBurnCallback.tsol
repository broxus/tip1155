pragma ever-solidity >= 0.58.0;

interface IMultiTokenBounceBurnCallback {
    /// @notice Callback from TokenWallet when tokens burn was reverted
    /// @param collection Collection of received tokens
    /// @param tokenId Unique token id
    /// @param count Reverted tokens count
    function onMultiTokenBounceBurn(
        address collection,
        uint256 tokenId,
        uint128 count
    ) external;
}
